{
    "name": "Archiver Dev Environment",
    "build": {
        "dockerfile": "Dockerfile",
        "context": ".",
        "args": {
            "BUILDKIT_INLINE_CACHE": "0"
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "golang.go",
                "ms-vscode.makefile-tools",
                "github.copilot",
                "github.copilot-chat"
            ],
            "settings": {
                "go.toolsManagement.checkForUpdates": "local",
                "go.useLanguageServer": true,
                "go.gopath": "/go",
                "go.formatTool": "gofmt",
                "editor.formatOnSave": true,
                "editor.codeActionsOnSave": {
                    "source.organizeImports": true
                }
            }
        }
    },
    "features": {
        "ghcr.io/devcontainers/features/github-cli:1": {}
    },
    "runArgs": [
        "--env-file", "${localWorkspaceFolder}/.env"
    ],
    "containerEnv": {
        "BUILDKIT_PROGRESS": "plain",
        "DOCKER_BUILDKIT": "0",
        "DEVCONTAINER_LOG_LEVEL": "debug"
    },
    "overrideCommand": false,
    "postStartCommand": "${containerWorkspaceFolder}/.devcontainer/entrypoint.sh",
    "mounts": [
        "source=devcontainer-logs,target=/var/log/devcontainer,type=volume"
    ],
    "postCreateCommand": "apt-get update && apt-get install -y ffmpeg sqlite3 libsqlite3-dev && go install honnef.co/go/tools/cmd/staticcheck@latest && go install github.com/greptile/greptile/cmd/greptile@latest && go mod download && go mod verify",
    "remoteUser": "vscode",
    "initializeCommand": "echo 'Starting devcontainer initialization at' $(date) > devcontainer-init.log"
}

